    <script data-wg-notranslate="">



        // ========================== Swatehes images  =========================

        document.addEventListener("DOMContentLoaded", function () {
            const swatchesBoxs = document.querySelectorAll(".swatchesBoxs_item");

            // Function to change images
            function changeImage(prod_rosaImages, index) {
                // Remove currentPic class from all images in this swatchesBox
                prod_rosaImages.forEach((img) => img.classList.remove("currentPic"));

                // Hide all images initially
                prod_rosaImages.forEach((img) => img.classList.add("hideOnload"));

                // Remove hideOnload to show the corresponding image and add currentPic class
                prod_rosaImages[index].classList.remove("hideOnload");
                prod_rosaImages[index].classList.add("currentPic");
            }

            swatchesBoxs.forEach((swatchesBox) => {
                let swatches = swatchesBox.querySelectorAll(".swatch > div");
                let prod_rosaImages = swatchesBox.querySelectorAll(".prod_rosa img");
                let clickedIndex = null; // Store the index of the clicked swatch

                swatches.forEach((swatch, index) => {
                    // Change image on mouse enter
                    swatch.addEventListener("mouseenter", () => {
                        changeImage(prod_rosaImages, index);
                    });

                    // Revert to clicked image on mouse leave, if there is one
                    swatch.addEventListener("mouseleave", () => {
                        if (clickedIndex !== null) {
                            changeImage(prod_rosaImages, clickedIndex);
                        }
                    });

                    // Change image and store index on click
                    swatch.addEventListener("click", function () {
                        clickedIndex = index; // Update the clicked index
                        changeImage(prod_rosaImages, index);
                        swatches.forEach((s) => s.classList.remove("selected"));
                        swatch.classList.add("selected");
                    });
                });
            });
        });






        // ========================== Accordion  Tabs =========================


        const items = document.querySelectorAll(".accor_dions button");

        function toggleaccor_dions() {
            const itemToggle = this.getAttribute("aria-expanded");

            for (i = 0; i < items.length; i++) {
                items[i].setAttribute("aria-expanded", "false");
            }

            if (itemToggle == "false") {
                this.setAttribute("aria-expanded", "true");
            }
        }

        items.forEach((item) => item.addEventListener("click", toggleaccor_dions));


        // ========================== Top Text sliders =========================



        window.onload = (event) => {
            let slideIndex = 0; // Start at slide index 0
            let slides = document.getElementsByClassName("mySlides");
            let autosSlideInterval;

            // Function to show a specific slide
            function showSlides(n) {
                // Hide all slides
                for (let i = 0; i < slides.length; i++) {
                    slides[i].classList.remove('active');
                }
                // Show the specified slide
                slides[n].classList.add('active');
            }

            // Function to change the slide when the prev/next buttons are clicked
            function plusSlides(n) {
                clearInterval(autosSlideInterval); // Stop the auto-slide on user interaction

                // Calculate the index of the next slide
                let nextIndex = (slideIndex + n + slides.length) % slides.length;

                // Hide current slide and show the next slide
                showSlides(nextIndex);

                // Update the slideIndex to the new slide
                slideIndex = nextIndex;

                // Restart the auto-slide after user interaction
                setAutoSlide();
            }

            // Function to auto-slide
            function setAutoSlide() {
                autosSlideInterval = setInterval(() => {
                    // Go to the next slide
                    plusSlides(1);
                }, 2000); // Slide every 4 seconds (4000ms)
            }

            // Attach the plusSlides function to the prev/next buttons
            const hosePrev = document.querySelector(".hose_prev");
            const hoseNext = document.querySelector(".hose_next");

            if (hosePrev && hoseNext) {
                hosePrev.addEventListener('click', () => plusSlides(-1));
                hoseNext.addEventListener('click', () => plusSlides(1));
                //showSlides(slideIndex);
                setAutoSlide();
            }
        };



        //  ======================= Pop Up Logic =========================




        document.addEventListener("DOMContentLoaded", function () {
            const isVisible = "is-visible";

            // Open popup
            const openEls = document.querySelectorAll("[data-open]");
            for (const el of openEls) {
                el.addEventListener("click", function (event) {
                    event.stopPropagation();  // Stop the event from bubbling up to higher elements
                    const modalId = this.dataset.open;
                    const modal = document.getElementById(modalId);
                    if (modal) {
                        modal.classList.add(isVisible);
                        modal.style.display = "block";  // Make sure to set display to block when opening
                    }
                });
            }

            // Close popup
            const closeEls = document.querySelectorAll("[data-close]");
            for (const el of closeEls) {
                el.addEventListener("click", function (event) {
                    event.stopPropagation();  // Prevent the event from bubbling up
                    const modalId = this.dataset.close;
                    const modal = document.getElementById(modalId);
                    if (modal) {
                        modal.classList.remove(isVisible);
                        modal.style.display = "none";  // Set display to none when closing
                    }
                });
            }

            // Close popup when clicking outside of the popup content
            document.addEventListener("click", (e) => {
                if (e.target.classList.contains("calcPopup") && e.target.classList.contains(isVisible)) {
                    e.target.classList.remove(isVisible);
                    e.target.style.display = "none";  // Set display to none when clicking outside
                }
            });

            // Close popup with the Escape key
            document.addEventListener("keyup", (e) => {
                if (e.key === "Escape" && document.querySelector(".calcPopup.is-visible")) {
                    const modal = document.querySelector(".calcPopup.is-visible");
                    modal.classList.remove(isVisible);
                    modal.style.display = "none";  // Set display to none with Escape key
                }
            });
        });



        //  ======================= Form Calculator logic in progress =========================


        var htmlLang = document.documentElement.lang;

        var currentLang = htmlLang ? htmlLang.substring(0, 2).toLowerCase() : "en";

        var sizeFormCm = document.getElementById("frm_size_calculator_cm");
        var heightCm = sizeFormCm.querySelector("#form-field-frm_size_height_cm");
        var bustCm = sizeFormCm.querySelector("#form-field-frm_size_bust_cm");
        var waistCm = sizeFormCm.querySelector("#form-field-frm_size_waist_cm");
        var hipsCm = sizeFormCm.querySelector("#form-field-frm_size_hips_cm");

        var sizeFormIn = document.getElementById("frm_size_calculator_in");
        var heightFt = sizeFormIn.querySelector("#form-field-frm_size_height_ft");
        var heightIn = sizeFormIn.querySelector("#form-field-frm_size_height_in");
        var bustIn = sizeFormIn.querySelector("#form-field-frm_size_bust_in");
        var waistIn = sizeFormIn.querySelector("#form-field-frm_size_waist_in");
        var hipsIn = sizeFormIn.querySelector("#form-field-frm_size_hips_in");

        var toggleInput = document.getElementById("unitsInput");

        var isProcessing = false;

        sizeFormCm.addEventListener("submit", function (event) {
            event.preventDefault();
            event.stopPropagation();

            if (isProcessing || toggleInput.checked) {
                return;
            }

            if (!heightCm || !bustCm || !waistCm || !hipsCm) {
                console.error("Missing input field(s).");
                isProcessing = false;
                return;
            }

            var heightValue = parseFloat(heightCm.value);
            var bustValue = parseFloat(bustCm.value);
            var waistValue = parseFloat(waistCm.value);
            var hipsValue = parseFloat(hipsCm.value);

            if (
                isNaN(heightValue) ||
                isNaN(bustValue) ||
                isNaN(waistValue) ||
                isNaN(hipsValue)
            ) {
                console.error("Invalid input value(s).");
                isProcessing = false;
                return;
            }

            calculateSize(heightValue, bustValue, waistValue, hipsValue);
            clearFormValues();
            isProcessing = false;
        });

        sizeFormIn.addEventListener("submit", function (event) {
            event.preventDefault();
            event.stopPropagation();

            if (isProcessing || !toggleInput.checked) {
                return;
            }

            if (!heightFt || !heightIn || !bustIn || !waistIn || !hipsIn) {
                console.error("Missing input field(s).");
                isProcessing = false;
                return;
            }

            var heightFtValue = parseFloat(heightFt.value);
            var heightInValue = parseFloat(heightIn.value);
            var bustValue = parseFloat(bustIn.value);
            var waistValue = parseFloat(waistIn.value);
            var hipsValue = parseFloat(hipsIn.value);

            if (
                isNaN(heightFtValue) ||
                isNaN(heightInValue) ||
                isNaN(bustValue) ||
                isNaN(waistValue) ||
                isNaN(hipsValue)
            ) {
                console.error("Invalid input value(s).");
                isProcessing = false;
                return;
            }

            calculateSize(
                (heightFtValue * 12 + heightInValue) * 2.54,
                bustValue * 2.54,
                waistValue * 2.54,
                hipsValue * 2.54
            );
            clearFormValues();
            isProcessing = false;
        });

        toggleInput.addEventListener("change", function () {
            if (!isProcessing) {
                toggleInput.disabled = true;
                isProcessing = true;

                if (this.checked) {
                    sizeFormCm.style.display = "none";
                    sizeFormIn.style.display = "block";
                } else {
                    sizeFormCm.style.display = "block";
                    sizeFormIn.style.display = "none";
                }

                convertValues(this.checked);

                clearFormValues();
                isProcessing = false;
                toggleInput.disabled = false;
            }
        });

        function getCookie(name) {
            var nameEQ = name + "=";
            var ca = document.cookie.split(";");
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == " ") c = c.substring(1, c.length);
                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
            }
            return null;
        }

        function setCookie(name, value, days = 7) {
            var expires = "";
            if (days) {
                var date = new Date();
                date.setTime(date.getTime() + days * 24 * 60 * 60 * 1000);
                expires = "; expires=" + date.toUTCString();
            }
            document.cookie = name + "=" + value + expires + "; path=/";
        }

        function calculateSize(heightValue, bustValue, waistValue, hipsValue) {
            var sizeChart = {
                bust: ["XXS", "XS", "S", "M", "L", "XL", "2XL", "3XL"],
                bustValues: [80, 85, 90, 95, 100, 108, 116, 124],
                waist: ["XXS", "XS", "S", "M", "L", "XL", "2XL", "3XL"],
                waistValues: [65, 70, 75, 80, 85, 95, 105, 115],
                hips: ["XXS", "XS", "S", "M", "L", "XL", "2XL", "3XL"],
                hipsValues: [90, 95, 100, 105, 110, 118, 126, 134],
            };

            function findClosestSizeUp(measurement, measurementsArray, sizesArray) {
                for (var i = 0; i < measurementsArray.length; i++) {
                    if (measurement <= measurementsArray[i]) {
                        return sizesArray[i];
                    }
                }
                return sizesArray[sizesArray.length - 1];
            }

            function findClosestSizeDown(measurement, measurementsArray, sizesArray) {
                for (var i = measurementsArray.length - 1; i >= 0; i--) {
                    if (measurement >= measurementsArray[i]) {
                        return sizesArray[i];
                    }
                }
                return sizesArray[0];
            }

            function findClosestSize(measurement, measurementsArray, sizesArray) {
                var closestValue = measurementsArray[0];
                var closestSize = sizesArray[0];

                for (var i = 0; i < measurementsArray.length; i++) {
                    if (
                        Math.abs(measurement - measurementsArray[i]) <
                        Math.abs(measurement - closestValue)
                    ) {
                        closestValue = measurementsArray[i];
                        closestSize = sizesArray[i];
                    }
                }

                return closestSize;
            }

            const topSizeBust = findClosestSizeUp(
                bustValue,
                sizeChart.bustValues,
                sizeChart.bust
            );
            const topSizeWaist = findClosestSize(
                waistValue,
                sizeChart.waistValues,
                sizeChart.waist
            );
            const topSizeHips = findClosestSize(
                hipsValue,
                sizeChart.hipsValues,
                sizeChart.hips
            );

            const pantsSizeWaist = findClosestSizeDown(
                waistValue,
                sizeChart.waistValues,
                sizeChart.waist
            );
            const pantsSizeHip = findClosestSizeDown(
                hipsValue,
                sizeChart.hipsValues,
                sizeChart.hips
            );

            const sizes = ["XXS", "XS", "S", "M", "L", "XL", "2XL", "3XL"];
            const topIndex = Math.max(
                sizes.indexOf(topSizeBust),
                sizes.indexOf(topSizeWaist),
                sizes.indexOf(topSizeHips)
            );
            const topSize = sizes[topIndex];

            const pantsIndex = sizes.indexOf(pantsSizeHip);
            const pantsSize = sizes[pantsIndex];

            var checkmarkSvg =
                '<svg aria-hidden="true" class="e-font-icon-svg e-far-check-circle" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg"><path fill="#34b233" d="M256 8C119.033 8 8 119.033 8 256s111.033 248 248 248 248-111.033 248-248S392.967 8 256 8zm0 48c110.532 0 200 89.451 200 200 0 110.532-89.451 200-200 200-110.532 0-200-89.451-200-200 0-110.532 89.451-200 200-200m140.204 130.267l-22.536-22.718c-4.667-4.705-12.265-4.736-16.97-.068L215.346 303.697l-59.792-60.277c-4.667-4.705-12.265-4.736-16.97-.069l-22.719 22.536c-4.705 4.667-4.736 12.265-.068 16.971l90.781 91.516c4.667 4.705 12.265 4.736 16.97.068l172.589-171.204c4.704-4.668 4.734-12.266.067-16.971z"></path></svg>';
            var warningSvg =
                '<svg aria-hidden="true" class="e-font-icon-svg e-fas-exclamation-triangle" viewBox="0 0 576 512" xmlns="http://www.w3.org/2000/svg"><path fill="#eed202" d="M569.517 440.013C587.975 472.007 564.806 512 527.94 512H48.054c-36.937 0-59.999-40.055-41.577-71.987L246.423 23.985c18.467-32.009 64.72-31.951 83.154 0l239.94 416.028zM288 354c-25.405 0-46 20.595-46 46s20.595 46 46 46 46-20.595 46-46-20.595-46-46-46zm-43.673-165.346l7.418 136c.347 6.364 5.609 11.346 11.982 11.346h48.546c6.373 0 11.635-4.982 11.982-11.346l7.418-136c.375-6.874-5.098-12.654-11.982-12.654h-63.383c-6.884 0-12.356 5.78-11.981 12.654z"></path></svg>';
            var suggestion =
                '<p><span class="rosaS_icon-list-icon" style="display: inline !important;">' +
                checkmarkSvg +
                "</span> Recommended Size: Top <strong>" +
                topSize +
                "</strong> | Pants <strong>" +
                pantsSize +
                "</strong>";

            if (heightValue < 155) {
                petiteWarning =
                    currentLang === "en"
                        ? "<strong>Attention: Petite Length Unavailable.</strong> Please be aware that our current sizes may have extra fabric at the ankle level. We apologize for any inconvenience."
                        : "<strong>Attention: Longueur Petite non disponible.</strong> Svp prendre note que nos pantalons pourraient être trop long au niveau des chevilles. Désolé de l'inconvénient";
                suggestion +=
                    '<br><span class="rosaS_icon-list-icon" style="display: inline !important;"> ' +
                    warningSvg +
                    "</span> " +
                    petiteWarning;
            } else if (heightValue > 175) {
                tallWarning =
                    currentLang === "en"
                        ? "<strong>Attention: Tall Length Unavailable.</strong> Please be aware that our current sizes may run short at the ankle level. We apologize for any inconvenience."
                        : "<strong>Attention: Longueur Tall non disponible.</strong> Svp prendre note que nos pantalons pourraient être trop court au niveau des chevilles. Désolé de l'inconvénient";
                suggestion +=
                    '<br><span class="rosaS_icon-list-icon" style="display: inline !important;"> ' +
                    warningSvg +
                    "</span> " +
                    tallWarning;
            }
            suggestion += "</p>";

            setTimeout(function () {
                var respDivId = toggleInput.checked
                    ? "div_size_suggestion_in"
                    : "div_size_suggestion_cm";
                var form = toggleInput.checked ? sizeFormIn : sizeFormCm;
                var responseDiv = document.getElementById(respDivId);

                if (!responseDiv) {
                    responseDiv = document.createElement("div");
                    responseDiv.id = respDivId;
                    responseDiv.className = "rosaS_field-group";
                    form
                        .querySelector(".rosaS_form-fields-wrapper")
                        .appendChild(responseDiv);
                }

                responseDiv.innerHTML = suggestion;
            }, 500);

            var expirationDate = new Date();
            expirationDate.setDate(expirationDate.getDate() + 30);

            var cookieTop = JSON.parse(getCookie("recommendedSizeTop") || "[]");
            var cookiePants = JSON.parse(getCookie("recommendedSizePants") || "[]");
            var cookieHeight = JSON.parse(getCookie("calcHeight") || "[]");
            var cookieBust = JSON.parse(getCookie("calcBust") || "[]");
            var cookieWaist = JSON.parse(getCookie("calcWaist") || "[]");
            var cookieHips = JSON.parse(getCookie("calcHips") || "[]");

            cookieTop.push(topSize);
            cookiePants.push(pantsSize);
            cookieHeight.push(heightValue);
            cookieBust.push(bustValue);
            cookieWaist.push(waistValue);
            cookieHips.push(hipsValue);

            setCookie("recommendedSizeTop", JSON.stringify(cookieTop));
            setCookie("recommendedSizePants", JSON.stringify(cookiePants));
            setCookie("calcHeight", JSON.stringify(cookieHeight));
            setCookie("calcBust", JSON.stringify(cookieBust));
            setCookie("calcWaist", JSON.stringify(cookieWaist));
            setCookie("calcHips", JSON.stringify(cookieHips));
        }

        function convertValues(isCm) {
            var cmValues = [
                [80, 85, 90, 95, 100, 108, 116, 124],
                [65, 70, 75, 80, 85, 95, 105, 115],
                [90, 95, 100, 105, 110, 118, 126, 134],
            ];
            var inchesValues = [
                [31.5, 33.5, 35.5, 37.5, 39.5, 42.5, 45.5, 49],
                [25.5, 27.5, 29.5, 31.5, 33.5, 37.5, 41.5, 45],
                [35.5, 37.5, 39.5, 41.5, 43.5, 46.5, 49.5, 53],
            ];

            var cells = document
                .getElementById("tbl_sizechart")
                .querySelectorAll("tbody td");

            cells.forEach((cell, index) => {
                var rowIndex = Math.floor(index / 8);
                cell.innerText = isCm
                    ? inchesValues[rowIndex][index % 8] + '"'
                    : cmValues[rowIndex][index % 8] + "cm";
            });
        }

        function clearFormValues() {
            heightCm.value = "";
            heightFt.value = "";
            heightIn.value = "";
            bustCm.value = "";
            bustIn.value = "";
            waistCm.value = "";
            waistIn.value = "";
            hipsCm.value = "";
            hipsIn.value = "";
        }

        function onTargetElementRendered() {
            document
                .getElementById("rosaS_popup-calcPopup-4795")
                .querySelector(".dialog-message")
                .scrollTo(0, 0);
        }

        function waitForTargetElement() {
            const targetElement = document.getElementById(
                "rosaS_popup-calcPopup-4795"
            );

            if (targetElement) {
                requestAnimationFrame(() => {
                    targetElement.offsetHeight;
                    onTargetElementRendered();
                });
            } else {
                const observer = new MutationObserver(() => {
                    const updatedTargetElement = document.getElementById(
                        "rosaS_popup-calcPopup-4795"
                    );
                    if (updatedTargetElement) {
                        observer.disconnect();
                        requestAnimationFrame(() => {
                            updatedTargetElement.offsetHeight;
                            onTargetElementRendered();
                        });
                    }
                });
                observer.observe(document.body, {
                    childList: true,
                    subtree: true,
                });
            }
        }

        waitForTargetElement();

        // ======================= Review Slider testimonial =============

        /*document.addEventListener("DOMContentLoaded", function () {
