    <script data-wg-notranslate="">
        document.addEventListener("DOMContentLoaded", function () {
            let welcomeNotice = document.getElementById('notice-welcome');
            let saleNotice1 = document.getElementById('notice-sale1');
            let saleNotice2 = document.getElementById('notice-sale2');
            let percentDiscount = null;

            function checkTimePeriod() {
                const now = new Date();
                const torontoTime = new Date(now.toLocaleString("en-US", { timeZone: "America/Toronto" }));

                // Christmas holidays 2024
                const startTime1 = new Date('December 19, 2024 00:00:00 EDT');
                const endTime1 = new Date('January 06, 2025 05:00:00 EDT');
                // Inactive
                const startTime2 = new Date('August 29, 2024 17:00:00 EDT');
                const endTime2 = new Date('September 4, 2024 04:00:00 EDT');

                if (torontoTime >= startTime1 && torontoTime <= endTime1) {
                    // Sale 1 active
                    percentDiscount = 25;
                    if (welcomeNotice) {
                        welcomeNotice.remove();
                        welcomeNotice = null;
                    }
                    if (saleNotice2) {
                        saleNotice2.remove();
                        saleNotice2 = null;
                    }
                } else if (torontoTime >= startTime2 && torontoTime <= endTime2) {
                    // Sale 2 active
                    percentDiscount = 30;
                    if (welcomeNotice) {
                        welcomeNotice.remove();
                        welcomeNotice = null;
                    }
                    if (saleNotice1) {
                        saleNotice1.remove();
                        saleNotice1 = null;
                    }
                } else {
                    // No active sale
                    if (saleNotice1) {
                        saleNotice1.style.display = 'none';
                        saleNotice1.remove();
                        saleNotice1 = null;
                    }
                    if (saleNotice2) {
                        saleNotice2.style.display = 'none';
                        saleNotice2.remove();
                        saleNotice2 = null;
                    }
                }
            }

            function applyDiscountDesign() {
                const productImgCtns = [
                    ...document.querySelectorAll('.swatchexPage'),
                    ...document.querySelectorAll('.productView-img-container')/*,
                ...document.querySelectorAll('.image-section')*/
                ];

                productImgCtns.forEach(c => {
                    const label = document.createElement('div');
                    label.textContent = `${percentDiscount}% OFF`;
                    label.className = 'discount-label';
                    Object.assign(label.style, {
                        position: 'absolute',
                        top: '5px',
                        left: '5px',
                        backgroundColor: '#ff8780',
                        color: '#ffffff',
                        padding: '3px 6px',
                        fontSize: '12px',
                        fontWeight: 'bold',
                        borderRadius: '3px',
                        zIndex: '10',
                    });
                    c.style.position = 'relative';
                    c.appendChild(label);
                });
            }

            checkTimePeriod();
            if (percentDiscount) {
                applyDiscountDesign();
            }
        });
    </script>
